<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
	targetNamespace="http://www.childmaintenance.gsi.gov.uk/futurescheme/bo/esb/eportal/CmecGetClientDetailsResponse"
	xmlns:bons0="http://www.childmaintenance.gsi.gov.uk/futurescheme/bo/esb/eportal/CmecEPortalContactDetails"
	xmlns:bons3="http://www.childmaintenance.gsi.gov.uk/futurescheme/bo/common/Response.xsd"
	xmlns:bons4="http://www.childmaintenance.gsi.gov.uk/futurescheme/bo/esb/eportal/CmecEPortalAddress"
	xmlns:bons6="http://www.childmaintenance.gsi.gov.uk/futurescheme/bo/esb/eportal/CmecEPortalHLSCaseDetails"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<xsd:import
		namespace="http://www.childmaintenance.gsi.gov.uk/futurescheme/bo/common/Response.xsd"
		schemaLocation="../../common/Response.xsd">
	</xsd:import>


	<xsd:import
		namespace="http://www.childmaintenance.gsi.gov.uk/futurescheme/bo/esb/eportal/CmecEPortalHLSCaseDetails"
		schemaLocation="CmecEPortalHLSCaseDetails.xsd" />
	<xsd:import
		namespace="http://www.childmaintenance.gsi.gov.uk/futurescheme/bo/esb/eportal/CmecEPortalContactDetails"
