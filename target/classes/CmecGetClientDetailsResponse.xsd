	<xsd:import
		namespace="http://www.childmaintenance.gsi.gov.uk/futurescheme/bo/esb/eportal/CmecEPortalAddress"
		schemaLocation="CmecEPortalAddress.xsd" />

	<xsd:complexType name="CmecGetClientDetailsResponse">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="ResponseHeader"
				type="bons3:Response">
			</xsd:element>
			<xsd:element maxOccurs="1" minOccurs="0"
				name="ClientDetails" type="bons0:CmecEPortalContactDetails">
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0"
				name="ClientAddress" type="bons4:CmecEPortalAddress">
			</xsd:element>


			<xsd:element maxOccurs="unbounded" minOccurs="0"
				name="CientHLSCaseDetails" type="bons6:CmecEPortalHLSCaseDetails">
			</xsd:element>


		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>